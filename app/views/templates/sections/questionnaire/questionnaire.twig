<div class="card-shadow">
    <h2>{{ tableTitle }}</h2>
    <form class="questionnaire-form">
        <div class="select-contact">
            <h3>
                <strong>
                    Forma kontaktu:
                </strong>
            </h3>
            <div class="select-contact__items">
                <div class="select-contact__item">
                    <input id="office" type="checkbox" />
                    <label for="office" class="fas fa-building"></label>
                </div>
                <div class="select-contact__item">
                    <input id="email" type="checkbox" />
                    <label for="email" class="fas fa-envelope-open-text"></label>
                </div>
                <div class="select-contact__item">
                    <input id="phone" type="checkbox" />
                    <label for="phone" class="fas fa-phone-volume"></label>
                </div>
                <div class="select-contact__item">
                    <input id="add-info" type="checkbox" />
                    <label for="add-info" class="fas fa-info"></label>
                </div>
            </div>
            <div class="select-contact__add-info">
                <input type="text" placeholder="Dodaj informacje">
                <button class="btn btn--small">zapisz</button>
            </div>
        </div>
        <div class="star-rating">
            <div class="star-rating__items">
                <div class="star-rating__item">
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Aenean non euismod ipsum.</p>
